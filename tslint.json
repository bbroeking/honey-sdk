{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "jsdoc-format": false,
        "ban-types": false,
        "no-bitwise": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/helpers/*.ts",
            "src/wrappers/util.ts"
        ]
    }
}